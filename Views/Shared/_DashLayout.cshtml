<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <title>WEBTEST | Dash</title>

    @Styles.Render("https://fonts.googleapis.com/css?family=Roboto:400,700")
    @Styles.Render("~/Content/Dash/css/fontawesome.min.css")
    @Styles.Render("~/Content/Dash/css/bootstrap.min.css")
    @Styles.Render("~/Content/Dash/css/templatemo-style.css")
</head>

<body id="reportsPage">
    <div class="" id="home">
        <nav class="navbar navbar-expand-xl">
            <div class="container h-100">
                <a class="navbar-brand" href="~">
                    <h1 class="tm-site-title mb-0">WEBTEST DASH</h1>
                </a>
                <button class="navbar-toggler ml-auto mr-0" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="fas fa-bars tm-nav-icon"></i>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mx-auto h-100">
                        <li class="nav-item">
                            <a class="nav-link active" href="@Url.Action("../Dash")">
                                <i class="fas fa-tachometer-alt"></i>
                                Dashboard
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        @*@if (Session["URID"].ToString() == "0")*@
                        @*{*@
                            <li class="nav-item dropdown">

                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                                    aria-haspopup="true" aria-expanded="false">
                                    <i class="far fa-file-alt"></i>
                                    <span>
                                        Products <i class="fas fa-angle-down"></i>
                                    </span>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    <a class="dropdown-item" href="@Url.Action("../Dash/CreatePrd")">Create Product</a>
                                    <a class="dropdown-item" href="@Url.Action("../Dash/ListPrd")">Show Products</a>
                                </div>
                            </li>
                        @*}*@
                        <li class="nav-item dropdown">

                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                                aria-haspopup="true" aria-expanded="false">
                                <i class="fas fa-shopping-cart"></i>
                                <span>
                                    Orders <i class="fas fa-angle-down"></i>
                                </span>
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item" href="@Url.Action("../Dash/CreateInv")">Create Order</a>
                                <a class="dropdown-item" href="@Url.Action("../Dash/ListInv")">Show Orders</a>
                            </div>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                <i class="far fa-user"></i>
                                Accounts
                            </a>
                        </li>
                    </ul>
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link d-block" href="@Url.Action("../LogOut")">
                                        @Session["UEmail"], <b>Logout</b>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>

                </nav>

                @RenderBody()


                <footer class="tm-footer row tm-mt-small">
                    <div class="col-12 font-weight-light">
                        <p class="text-center text-white mb-0 px-4 small">
                            Copyright &copy; <b>2021</b> All rights reserved.
                            Developed : <a rel="nofollow noopener" href="https://www.facebook.com/MMMolayElHassan" class="tm-footer-link">ABOUSSALAM Hassan</a>
                        </p>
                    </div>
                </footer>
            </div>


            @Scripts.Render("~/Content/Dash/js/jquery-3.3.1.min.js")
            @Scripts.Render("~/Content/Dash/js/moment.min.js")
            @Scripts.Render("~/Content/Dash/js/Chart.min.js")
            @Scripts.Render("~/Content/Dash/js/bootstrap.min.js")
            @Scripts.Render("~/Content/Dash/js/tooplate-scripts.js")
            <script>
              $(function() {
                $(".tm-product-name").on("click", function() {
                  window.location.href = "edit-product.html";
                });
              });
            </script>

        </body>
        </html>
